{
  "name": "uranio-client",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc -b",
    "dev": "npx tsc -w",
    "dev:base": "node -r source-map-support/register -r dotenv/config ./dist/run.js",
    "dev:run": "npx tsc-watch --onSuccess \"npm run dev:base\"",
    "dev:test": "node --import tsx --test --test-reporter spec --watch ./tests/*.test.ts",
    "test": "npx jest",
    "test:jest:watch": "npx jest --watch",
    "test:jest:coverage": "npx jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "dotenv": "^16.3.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-json": "^4.0.1",
    "jest": "^30.2.0",
    "prettier": "3.8.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.6",
    "tsc-watch": "^7.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.995.0",
    "@aws-sdk/util-dynamodb": "^3.995.0",
    "mongodb": "^7.1.0",
    "mysql2": "^3.17.5",
    "pg": "^8.18.0",
    "uranio": "1.1.8"
  }
}
